    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <MFXButton id="custom" buttonType="RAISED" defaultButton="true" onAction="#botonPrueba" stylesheets="@../css/estilos.css" text="Hello!" textAlignment="CENTER" />
